<table>
  <tr>
    <th>Rank</th>
    <th>Name</th>
    <th>Time</th>
    <th>Gear Points</th>
  </tr>
  <% data.forEach((item, index)=> { %>
  <tr>
    <td style="text-align: right;">
      <%= index + 1 %>.
    </td>
    <td>
      <%= item.name %>
    </td>
    <td style="text-align: right;">
      <%= item.time %>
    </td>
    <td style="text-align: right;">
      <%= item.power %>
    </td>
  </tr>
  <% }); %>
</table>